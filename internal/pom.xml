    <groupId>net.flawe.offlinemanager</groupId>
    <artifactId>internal</artifactId>
    <name>OfflineManagerAdapter</name>

    <packaging>pom</packaging>

    <profiles>
        <profile>
            <id>all</id>
            <modules>
